<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.ncmashd.mapper.UserMapper">
    <insert id="register">
        insert into user (username,password,avatar,nickname,Permissions)
        values (#{username},#{password},#{avatar},#{nickname},#{Permissions})
    </insert>
    <select id="selectByUsername" resultType="com.example.ncmashd.entity.User">
        select * from user where username = #{username}
    </select>
</mapper>